{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/hours/(.*)",
        "dest": "/api/hours/[date].js?date=$1"
      },
      {
        "src": "/api/goals",
        "dest": "/api/goals.js"
      },
      {
        "src": "/",
        "dest": "public/pages/index.html"
      },
      {
        "src": "/(record|view|analytics|goals)",
        "dest": "public/pages/$1.html"
      },
      {
        "src": "/(css|js)/(.*)",
        "dest": "public/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }